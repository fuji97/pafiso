{
  "name": "pafiso",
  "version": "1.0.0",
  "description": "TypeScript library for generating Paging, Filtering, and Sorting parameters for REST requests",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "paging",
    "filtering",
    "sorting",
    "query-string",
    "rest-api",
    "queryable"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.19.6",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.17"
  }
}
